import javax.swing.JLabel;
import javax.swing.JTextField;

class SwingDemo{
	JButton b1,b2,b3,b4;
	JTextField t1,t2,t3,t4;
	JLabel l1,l2,l3,l4;
	public SwingDemo() {
		JFrame fr = new JFrame("AppName");
		fr.setVisible(true);
		fr.setSize(700, 500);
		
		
	}
}
public class P028_Swing {
	public static void main(String[] args) {
		new SwingDemo();
	}
}
