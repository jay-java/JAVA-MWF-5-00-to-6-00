package core;
class Employee{
	int id;
	String name;
	double salary;
	Employee(){
		System.out.println("default cons");
	}
	Employee(int id,String name,double salary){
		
	}
	public void show() {
		System.out.println("show in employee");
	}
}
public class P010_Constructor {
	public static void main(String[] args) {
		Employee e1 = new Employee();
		e1.show();
	}
}
