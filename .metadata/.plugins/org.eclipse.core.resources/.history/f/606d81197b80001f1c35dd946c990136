package core;

import java.util.HashMap;
import java.util.Map;

public class P026_Map {
	public static void main(String[] args) {
		Map map = new HashMap();
		map.put(1, "c");
		map.put(2, "c++");
		map.put("java", "c");
		map.put(454.3, "python");
		map.put(false, "kotlin");
		map.put(6, "dart");
		map.put(1, "php");
		System.out.println(map);
	}
}
